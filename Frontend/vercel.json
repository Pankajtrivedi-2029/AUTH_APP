{
    "version": 2,
   "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static",
        "config":{
            "includeFiles" : [
                "dist/**"
            ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  
