{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static",
        "config": {
          "includeFiles": [
            "dist/**"
          ]
        }
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "script-src 'self' 'unsafe-inline' 'unsafe-eval' https://cdn.jsdelivr.net"
          }
        ]
      }
    ]
  }
  